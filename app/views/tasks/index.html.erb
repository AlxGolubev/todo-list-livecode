<h1>Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Related to List</th>
      <th>Completed</th>
      <th colspan="3">Acitons</th>
    </tr>
  </thead>
  <tbody>
    <% @tasks.each do |task| %>
      <tr>
        <td><%= task.title %></td>
        <td><%= task.list.title %>
        <td><%= task.completed %></td>
        <td>Links will be there</td>
      </tr>
    <% end %>
  </tbody>

